{
  "name": "veda-jewelry",
  "version": "0.0.4",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/timbenniks/veda-the-revival-collection"
  },
  "description": "Veda Jewelry Contentstack demo",
  "author": "Tim Benniks <tim.benniks@contentstack.com>",
  "homepage": "https://contentstack.com",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsgen": "npx @contentstack/cli tsgen -o ./types/contentstack.d.ts -a delivery --no-doc --include-system-fields",
    "update-launch-config": "node updateLaunchConfig.mjs",
    "prebuild": "npm run update-launch-config",
    "install-browsers": "npx ts-node --project scripts/traffic-simulation/tsconfig.json scripts/traffic-simulation/install-browsers.ts",
    "simulate-traffic": "npx ts-node --project scripts/traffic-simulation/tsconfig.json scripts/traffic-simulation/simulate-visitors.ts",
    "simulate-traffic:headless": "cross-env HEADLESS=true npx ts-node --project scripts/traffic-simulation/tsconfig.json scripts/traffic-simulation/simulate-visitors.ts",
    "simulate-profiles": "./scripts/traffic-simulation/run-profiles.sh",
    "simulate-profiles:install": "./scripts/traffic-simulation/run-profiles.sh --install-browsers"
  },
  "dependencies": {
    "@contentstack/delivery-sdk": "^4.7.0",
    "@contentstack/live-preview-utils": "^3.2.2",
    "@contentstack/personalize-edge-sdk": "^1.0.15",
    "@timbenniks/contentstack-endpoints": "^1.0.10",
    "@uiw/react-json-view": "^2.0.0-alpha.32",
    "algoliasearch": "^5.25.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-instantsearch": "^7.15.8",
    "react-instantsearch-nextjs": "^0.4.9",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^22.15.24",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/xml2js": "^0.4.12",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "xml2js": "^0.6.2"
  }
}
